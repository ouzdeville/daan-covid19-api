  "apidoc": {
    "name": "daan-covid19-api",
    "version": "1.0.0",
    "description": "apiDoc daan-covid19-api",
    "title": "apiDoc daan-covid19-api",
    "url": "https://daan-covid19-api.herokuapp.com"
  },
  "main": "index.js",
  "scripts": {
    "start:dev": "nodemon ./bin/www",
    "doc": "apidoc -i controller/ -o public/apidoc/",
    "start": "npm run doc && node ./bin/www",
    "deploy:production": "claudia update --set-env-from-json prodEnv.json NODE_ENV=production --version production",
    "deploy:production:create": "claudia create  --set-env-from-json prodEnv.json NODE_ENV=production --version production --handler lambda.handler --deploy-proxy-api --region us-east-1",
    "deploy": "pm2 restart index.js --name daan-covid19-api",
    "migrate:reset": "NODE_ENV=test npx sequelize-cli db:migrate:undo:all && npm run migrate",
    "test": "NODE_ENV=test jest --testTimeout=10000 --detectOpenHandles",
    "pretest": "NODE_ENV=test npm run migrate:reset",
    "migrate": "npx sequelize-cli db:migrate"
  },
